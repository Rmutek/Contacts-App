<h1>Create a New Contact!</h1>
<%= form_tag "/contact_form_results_url", method: :post do %> 
<div>First name: <input type="text" name="first_name"></div>
<div>Last name: <input type="text" name="last_name"></div>
<div>Email: <input type="text" name="email"></div>
<div>Phone number: <input type="text" name="phone_number"></div>
<input type="submit" value="Create Contact!">
<% end %>
